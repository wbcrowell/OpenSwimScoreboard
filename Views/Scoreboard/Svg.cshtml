@using OpenSwimScoreboard.Parameters
<script>
    var numLanes = @(DisplaySettings.NumLanes);
    var scoreboardNames = null;
    fetch("/ScoreboardFiles/session.json")
        .then((response) => {
            return response.json();
        })
        .then((data) => {
            scoreboardNames = data;
        });
</script>
<div class="positioner">
    <div class="background float-right" style="width:40%">
        <div class="container">
            <div class="row header-row">
                <div class="col-12">
